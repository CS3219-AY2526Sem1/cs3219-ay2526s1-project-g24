    "@opentelemetry/resources": "^1.27.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "ioredis": "^5.4.1",
    "jose": "^5.9.2",
    "pino": "^9.5.0",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
